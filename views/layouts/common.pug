  main
    .wrapper
      //- Sidebar
      nav#sidebar
        .sidebar-header.d-flex.justify-content-between
          a.nav__logo(href='/')
          button#sidebarCollapse.btn.btn-info(type='button')
            i.fas.fa-bars#nav__menu
            //-span#nav__close &times
        ul.list-unstyled.components
          li.active
            a.dropdown-toggle(href='/')
              i.fas.fa-home
              span Home
          li
            a(href='#')
              i.fas.fa-briefcase
              span About
            a.dropdown-toggle(href='#pageSubmenu', data-toggle='collapse', aria-expanded='false')
              i.fas.fa-copy
              span Pages
            ul#pageSubmenu.collapse.list-unstyled
              li
                a(href='#') Page 1
              li
                a(href='#') Page 2
              li
                a(href='#') Page 3
        form.form-inline.my-auto
          .dropdown(class= isUserLogin ? 'd-block' : 'd-none')
            i.fas.fa-user-circle.dropdown-toggle(id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false")
            .dropdown-menu(aria-labelledby="dropdownMenuButton")
              p.dropdown-item
                i.fas.fa-user-cog
                a(href="/") Setting
              a.dropdown-item.btn.btn-danger.font-weight-bold(href="/logout", class= isUserLogin ? 'd-block' : 'd-none')
                span.glyphicon.glyphicon-user
                |Sign out
          a.btn.btn-success.font-weight-bold(href="/login", class= isUserLogin ? 'd-none' : 'd-block')
            span.glyphicon.glyphicon-user
            |Sign in
          a.btn.btn-warning.font-weight-bold.ml-1(href="/login", class= isUserLogin ? 'd-none' : 'd-block')
            span.glyphicon.glyphicon-user
            |Sign up
      //- Page Content
      #content
        block content
        block pagination
          .container
            .row
              nav.mt-4(aria-label='Page navigation example')
                ul.pagination
                  //- pre
                  if page == 1
                    li.page-item.disabled
                      a.page-link(href='#')
                        span &laquo;
                  else
                    li.page-item
                      a.page-link(href=route + page5Num[1])
                        span &laquo;
                  //- 123
                  if totalPage <= 1
                    li.page-item.active.disabled
                      a.page-link(href=route + val) 1
                  else if totalPage == 2
                    each val in [1, 2]
                      if val == page
                        li.page-item.active.disabled
                          a.page-link(href=route + val)= val
                      else
                        li.page-item
                          a.page-link(href=route + val)= val
                  else if page == totalPage
                    each val in [page5Num[0],page5Num[1],page5Num[2]]
                      if val == totalPage
                        li.page-item.active.disabled
                          a.page-link(href=route + val)= val
                      else
                        li.page-item
                          a.page-link(href=route + val)= val
                  else
                    each val in [page5Num[1], page5Num[2], page5Num[3]]
                      if val == page
                        li.page-item.active.disabled
                          a.page-link(href=route + val)= val
                      else
                        li.page-item
                          a.page-link(href=route + val)= val
                  //- next
                  if page == totalPage || totalPage <= 1
                    li.page-item.disabled
                      a.page-link(href='#')
                        span &raquo;
                  else
                    li.page-item
                      a.page-link(href=route + page5Num[3])
                        span &raquo;
